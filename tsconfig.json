{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "typeRoots": [
        "./node_modules/@types"
      ],
      "types": [
        "node"
      ],
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
          ]
      }
  },
  "include": [
      "src/**/*"
  ]
}